{
  "manifest_version": 2,
  "name": "FireX Proxy",
  "version": "5.1.9",
  "description": "FireX Proxy",
  "default_locale": "en",
  "icons": {
    "16": "data/icons/action/icon-16.png",
    "32": "data/icons/action/icon-32.png",
    "48": "data/icons/action/icon-48.png",
    "64": "data/icons/action/icon-64.png",
    "128": "data/icons/action/icon-128.png"
  },
  "applications": {
    "gecko": {
      "id": "divanproger@gmail.com",
      "strict_min_version": "57.0"
    }
  },
  "permissions": [
    "proxy",
    "storage",
    "*://firexproxy.com/*"
  ],
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "data/icons/action/icon-16.png",
      "32": "data/icons/action/icon-32.png",
      "48": "data/icons/action/icon-48.png",
      "64": "data/icons/action/icon-64.png",
      "128": "data/icons/action/icon-128.png"
    },
    "default_title": "FireX Proxy",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "addon/browser-polyfill.js",
      "addon/address.js",
      "addon/addresses.js",
      "addon/connector.js",
      "addon/helpers.js",
      "addon/proxyProvider.js",
      "addon/background.js"
    ]
  }
}

{
  "manifest_version": 2,
  "name": "FireX Proxy",
  "version": "5.1.3",
  "description": "FireX Proxy",
  "default_locale": "en",
  "icons": {
    "16": "data/icons/icon-16.png",
    "24": "data/icons/icon-24.png",
    "32": "data/icons/icon-32.png"
  },
  "applications": {
    "gecko": {
      "id": "divanproger@gmail.com",
      "strict_min_version": "57.0"
    }
  },
  "permissions": [
    "proxy",
    "storage",
    "*://firexproxy.com:4040/*"
  ],
  "browser_action": {
    "browser_style": false,
    "default_icon": "data/icons/icon-32.png",
    "default_title": "FireX Proxy",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "addon/build/index.js",
      "addon/build/pac.js"
    ]
  }
}
